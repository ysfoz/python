    "    a=1\n",
    "    b=1\n",
    "    if x ==1:\n",
    "        return x\n",
    "    else:\n",
    "        while x > 2:\n",
    "            a,b = b,a+b\n",
    "            x-=1\n",
    "        return a+b\n",
    "    "
