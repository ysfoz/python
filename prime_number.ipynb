   "source": [
    "value = input('enter a number')\n",
    "number = int(value)\n",
    "counter = 0\n",
    "for i in range(2,number):\n",
    "    if not(number % i) != 0:\n",
    "        counter += 1\n",
    "if counter == 0 :\n",
    "    print('this is a prime number')\n",
    "else:\n",
    "    print('this is not a prime number')\n",
    "                  "
